int fib(int n);

int main(int argc)
{
    write(fib(argc));
    return 0;
}

int fib(int n)
{
    if (n < 2)
        return 1;
    else;

    int prev;   prev = 1;
    int cur;    cur  = 1;

    int i;      i = 1;      // index of fib(cur)
    while (i < n)
    {
        int next;   next = prev + cur;
        prev = cur;
        cur  = next;
        i = i + 1;
    };

    return cur;
}
